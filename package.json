  },
  "devDependencies": {
    "eclint": "^2.8.1",
    "updates": "^13.1.4",
    "versions": "^9.3.0"
  },
  "scripts": {
    "authors": "bash tools/authors.sh",
    "patch": "versions -p -C patch dd-wrt-forum-inspired-themes.user.styl && git psh",
    "minor": "versions -p -C minor dd-wrt-forum-inspired-themes.user.styl && git psh",
    "major": "versions -p -C major dd-wrt-forum-inspired-themes.user.styl && git psh",
    "fix": "eclint fix dd-wrt-forum-inspired-themes.user.styl && eclint fix *.md",
    "update": "updates -cuU && npm install"
  }
